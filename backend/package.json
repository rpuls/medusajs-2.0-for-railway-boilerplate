{
  "name": "medusa-2.0-boilerplate-backend",
  "version": "0.0.2",
  "description": "A Prebaked Medusa 2.0 backend",
  "author": "Rasmus Puls",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "build": "medusa build && node src/scripts/postBuild.js",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "ib": "init-backend",
    "start": "init-backend && medusa exec ./src/scripts/ensure-migrations.ts && cd .medusa/server && medusa start --verbose",
    "dev": "medusa develop",
    "email:dev": "email dev --dir=./src/modules/email-notifications/templates --port=3002"
  },
  "dependencies": {
    "@agilo/medusa-analytics-plugin": "^1.2.1",
    "@medusajs/admin-sdk": "2.12.3",
    "@medusajs/admin-shared": "2.12.3",
    "@medusajs/cli": "2.12.3",
    "@medusajs/dashboard": "2.12.3",
    "@medusajs/draft-order": "^2.12.3",
    "@medusajs/framework": "2.12.3",
    "@medusajs/icons": "2.12.3",
    "@medusajs/medusa": "2.12.3",
    "@medusajs/notification-sendgrid": "2.12.3",
    "@medusajs/payment-stripe": "2.12.3",
    "@medusajs/workflow-engine-redis": "2.12.3",
    "@medusajs/workflows-sdk": "^2.12.3",
    "@mikro-orm/core": "6.4.16",
    "@mikro-orm/knex": "6.4.16",
    "@mikro-orm/migrations": "6.4.16",
    "@mikro-orm/postgresql": "6.4.16",
    "@react-email/components": "^0.0.26",
    "@react-email/render": "^2.0.0",
    "@rokmohar/medusa-plugin-meilisearch": "1.3.5",
    "@sendgrid/mail": "^8.1.6",
    "awilix": "^8.0.1",
    "fast-xml-parser": "^4.3.5",
    "medusajs-launch-utils": "0.0.18",
    "minio": "^8.0.3",
    "pg": "^8.13.1",
    "resend": "4.0.1",
    "ulid": "^2.3.0",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "@medusajs/test-utils": "2.12.3",
    "@mikro-orm/cli": "6.4.16",
    "@swc/core": "1.5.7",
    "@swc/jest": "^0.2.37",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.10",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "jest": "^29.7.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-email": "^3.0.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "vite": "^5.4.11"
  },
  "packageManager": "pnpm@9.10.0",
  "engines": {
    "node": "22.x",
    "npm": "10.x"
  }
}
