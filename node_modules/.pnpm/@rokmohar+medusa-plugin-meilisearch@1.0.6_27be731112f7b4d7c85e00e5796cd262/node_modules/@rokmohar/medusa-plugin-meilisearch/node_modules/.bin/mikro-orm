#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/angassmith/Documents/medusajs-2.0-for-railway-boilerplate/node_modules/.pnpm/@mikro-orm+cli@6.4.11_pg@8.14.1/node_modules/@mikro-orm/cli/node_modules:/Users/angassmith/Documents/medusajs-2.0-for-railway-boilerplate/node_modules/.pnpm/@mikro-orm+cli@6.4.11_pg@8.14.1/node_modules/@mikro-orm/node_modules:/Users/angassmith/Documents/medusajs-2.0-for-railway-boilerplate/node_modules/.pnpm/@mikro-orm+cli@6.4.11_pg@8.14.1/node_modules:/Users/angassmith/Documents/medusajs-2.0-for-railway-boilerplate/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/angassmith/Documents/medusajs-2.0-for-railway-boilerplate/node_modules/.pnpm/@mikro-orm+cli@6.4.11_pg@8.14.1/node_modules/@mikro-orm/cli/node_modules:/Users/angassmith/Documents/medusajs-2.0-for-railway-boilerplate/node_modules/.pnpm/@mikro-orm+cli@6.4.11_pg@8.14.1/node_modules/@mikro-orm/node_modules:/Users/angassmith/Documents/medusajs-2.0-for-railway-boilerplate/node_modules/.pnpm/@mikro-orm+cli@6.4.11_pg@8.14.1/node_modules:/Users/angassmith/Documents/medusajs-2.0-for-railway-boilerplate/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@mikro-orm+cli@6.4.11_pg@8.14.1/node_modules/@mikro-orm/cli/cli" "$@"
else
  exec node  "$basedir/../../../../../../@mikro-orm+cli@6.4.11_pg@8.14.1/node_modules/@mikro-orm/cli/cli" "$@"
fi
