#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/angassmith/Documents/medusajs-2.0-for-railway-boilerplate/node_modules/.pnpm/vite@5.4.17_@types+node@22.14.0/node_modules/vite/bin/node_modules:/Users/angassmith/Documents/medusajs-2.0-for-railway-boilerplate/node_modules/.pnpm/vite@5.4.17_@types+node@22.14.0/node_modules/vite/node_modules:/Users/angassmith/Documents/medusajs-2.0-for-railway-boilerplate/node_modules/.pnpm/vite@5.4.17_@types+node@22.14.0/node_modules:/Users/angassmith/Documents/medusajs-2.0-for-railway-boilerplate/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/angassmith/Documents/medusajs-2.0-for-railway-boilerplate/node_modules/.pnpm/vite@5.4.17_@types+node@22.14.0/node_modules/vite/bin/node_modules:/Users/angassmith/Documents/medusajs-2.0-for-railway-boilerplate/node_modules/.pnpm/vite@5.4.17_@types+node@22.14.0/node_modules/vite/node_modules:/Users/angassmith/Documents/medusajs-2.0-for-railway-boilerplate/node_modules/.pnpm/vite@5.4.17_@types+node@22.14.0/node_modules:/Users/angassmith/Documents/medusajs-2.0-for-railway-boilerplate/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/vite@5.4.17_@types+node@22.14.0/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../.pnpm/vite@5.4.17_@types+node@22.14.0/node_modules/vite/bin/vite.js" "$@"
fi
